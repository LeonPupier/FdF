# Constants
NAME		=	fdf
CFLAGS		=	-Wall -Wextra -Werror #-fsanitize=address -g3

# Directories path
DIR_SRCS	=	sources/
DIR_HEADERS	=	headers/
DIR_LIBFT	=	libft/
DIR_MLX		=	mlx/

# Files path
HEADERS	=	$(DIR_HEADERS)fdf.h \
			$(DIR_LIBFT)libft.h \
			$(DIR_LIBFT)ft_printf.h \
			$(DIR_LIBFT)get_next_line.h

SRCS	=	$(DIR_SRCS)main.c \
			$(DIR_SRCS)map.c \
			$(DIR_SRCS)parser.c \
			$(DIR_SRCS)geometry.c \
			$(DIR_SRCS)events.c \
			$(DIR_SRCS)utils.c

OBJS	=	$(SRCS:.c=.o)

# Rules
.PHONY :	all re clean fclean libft mlx mlx_clean mickey anonymous

all :		${NAME}

%.o: %.c	$(HEADERS) Makefile
			$(CC) $(CFLAGS) -Imlx/ -c $< -o $@

${NAME}:	mlx libft ${OBJS}
			$(CC) $(CFLAGS) $(OBJS) $(DIR_LIBFT)libft.a -Lmlx -lmlx -framework OpenGL -framework AppKit -o $(NAME)

clean :
			${RM} ${OBJS}
			make clean -C $(DIR_LIBFT)

fclean :	clean
			${RM} ${NAME} 
			make fclean -C $(DIR_LIBFT)

re :		fclean all

libft:
	make -C $(DIR_LIBFT)

mlx:
	make -C $(DIR_MLX)

mlx_clean:
	make clean -C $(DIR_MLX)

mickey:
			@echo "$$MICKEY"

anonymous:
			@echo "$$ANONYMOUS"

# Text art
